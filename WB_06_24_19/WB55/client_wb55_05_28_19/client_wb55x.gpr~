   for Target use "arm-eabi";

   for Runtime ("Ada") use STM32WB55x_Nucleo'Runtime("Ada");

   package Builder is
     for Global_Configuration_Pragmas use "gnat.adc";
   end Builder;

end ipcc_wb55x;
